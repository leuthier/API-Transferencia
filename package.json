{
  "name": "api-testes-express",
  "version": "1.0.0",
  "description": "API para aprendizado de testes e automação - Express, memória, Swagger. Esta API permite o registro, login, consulta de usuários e transferências de valores entre usuários.",
  "main": "server.js",
  "scripts": {
  "test": "mocha test/**/*.test.js --timeout 10000 --reporter mochawesome",
  "test-rest-external": "mocha test/external/rest/*.test.js --timeout 10000 --reporter mochawesome",
  "test-rest-controller": "mocha test/controller/rest/*.test.js --timeout 10000 --reporter mochawesome",
  "test-graphql-external": "mocha test/external/graphql/*.test.js --timeout 10000 --reporter mochawesome",
  "start-rest": "node server.js",
  "start-graphql": "node graphql/server.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "apollo-server-express": "^3.13.0",
    "chai": "4.5.0",
    "express": "^4.21.2",
    "graphql": "^16.11.0",
    "jsonwebtoken": "^9.0.2",
    "mocha": "^11.7.1",
    "mochawesome": "^7.1.3",
    "sinon": "^21.0.0",
    "supertest": "^7.1.4"
  }
}
